{
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "browser": true,
      "jquery": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-unused-vars": 1,
      "curly": 2,
      "indent": [2, 2, { "SwitchCase": 1 }],
      "eqeqeq": 2,
      "new-cap": 2,
      "camelcase": 2,
      "quotes": [2, "single"],
      "no-trailing-spaces": 2
    }
  }
  